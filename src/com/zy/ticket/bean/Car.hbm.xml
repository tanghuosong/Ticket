<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zy.ticket.bean.Car" table="car">
    	<id name="id" column="id" type="int">
    		<generator class="native"/>
    	</id>
    	<property name="carName" 		column="carName" 			type="string"/>
    	<property name="carCard" 		column="carCard" 			type="string"/>
    	<property name="buyTime" 		column="buyTime" 			type="timestamp"/>
    	<property name="seatNum" 		column="seatNum"		 	type="int"/>
    	<many-to-one name="carType" class="com.zy.ticket.bean.CarType" 
    			column="carTypeId" not-null="true" 
    		 />
    </class>
</hibernate-mapping>
